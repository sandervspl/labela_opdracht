@import url('https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700')

*
    margin 0
    padding 0
    box-sizing border-box;

html, body
    width 100%
    height 100%
    color #000

body
    font-family 'Roboto', Helvetica, Arial, sans-serif
    background-color #eee

a
    text-decoration none

ul li
    list-style none